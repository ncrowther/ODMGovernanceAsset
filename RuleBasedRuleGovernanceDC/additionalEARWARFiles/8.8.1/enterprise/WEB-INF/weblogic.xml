<weblogic-web-app xmlns="http://www.bea.com/ns/weblogic/90" xmlns:j2ee="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/ns/weblogic/90 http://www.bea.com/ns/weblogic/90/weblogic-web-app.xsd">

	<security-role-assignment>
		<role-name>rtsUser</role-name>
		<externally-defined />
	</security-role-assignment>

	<security-role-assignment>
		<role-name>rtsAdministrator</role-name>
		<externally-defined />
	</security-role-assignment>

	<security-role-assignment>
		<role-name>rtsConfigManager</role-name>
		<externally-defined />
	</security-role-assignment>

	<security-role-assignment>
		<role-name>rtsInstaller</role-name>
		<externally-defined />
	</security-role-assignment>
	
	<security-role-assignment>
		<role-name>authorL1</role-name>
		<externally-defined />
	</security-role-assignment>
	
	<security-role-assignment>
		<role-name>authorL2</role-name>
		<externally-defined />
	</security-role-assignment>
	
	<security-role-assignment>
		<role-name>tester</role-name>
		<externally-defined />
	</security-role-assignment>
	
	<security-role-assignment>
		<role-name>reviewer</role-name>
		<externally-defined />
	</security-role-assignment>
		
      
      <resource-description>
   <res-ref-name>eis/XUConnectionFactory</res-ref-name>
   <jndi-name>eis/XUConnectionFactory</jndi-name>
</resource-description>		

      <resource-description>
   <res-ref-name>mail/RuleGovMailSession</res-ref-name>
   <jndi-name>mail/RuleGovMailSession</jndi-name>
</resource-description>	

	<session-descriptor>
		<cookie-name>RuleTeamServerCookie</cookie-name>
	</session-descriptor>

	<container-descriptor>
		<prefer-web-inf-classes>true</prefer-web-inf-classes>
		<relogin-enabled>true</relogin-enabled>
	</container-descriptor>
	    
	
</weblogic-web-app>
